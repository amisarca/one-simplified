{
	"name": "One Andrei",
	"type": "dark",
	"colors": {
		"editor.background": "#282C34",
		"editor.foreground": "#A9B7C6",
		"activityBarBadge.background": "#007acc",
		"statusBar.background" : "#303030",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#59626F"
			}
		},{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"variable.language.this",
				"constant.language"
			],
			"settings": {
				"foreground": "#C679DD"
			}
		},{
			"name": "Normal foreground",
			"scope": [
				"keyword.operator.assignment",
				"storage.type.java",
				"storage.type.generic.java",
				"storage.modifier.import.java",
				"storage.modifier.package.java",
				"punctuation.definition",
				"punctuation.section",
				"punctuation.separator.delimiter.",
				"punctuation.bracket.angle",
				"variable.parameter.java",
				"variable.other.object.java",
				"constant.other.key.java"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},{
			"name": "Instance variable",
			"scope": [
				"variable.other.definition.java",
				"variable.other.object.property",
				"variable.other.property",
				"variable.other.object",
				"entity.name.function.call.initializer.cpp",
			],
			"settings": {
				"foreground": "#FDA5FF"
			}
		},{
			"name": "Class variable",
			"scope": [
				"meta.enum",
				"variable.other.constant",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FDA5FF"
			}
		},{
			"name": "Annotation",
			"scope": [
				"punctuation.definition.annotation.java",
				"storage.type.annotation.java",
			],
			"settings": {
				"foreground": "#BBB529"
			}
		},{
			"name": "Function name",
			"scope": [
				"meta.method.identifier",
				"meta.definition.method",
				"entity.other.attribute-name",
				"entity.name.function.definition.cpp",
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},{
			"name": "String",
			"scope": [
				"punctuation.definition.string",
				"string",
			],
			"settings": {
				"foreground": "#98C379"
			}
		},{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#E26B73"
			}
		},{
			"name": "Number",
			"scope": [
				"constant.numeric.decimal",
			],
			"settings": {
				"foreground": "#D2945D"
			}
		},
	]
}